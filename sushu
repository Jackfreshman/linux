#include <stdio.h>

#include<math.h>

void main()

{   int n,i,j;

    printf("please input a integer:\n");

    scanf("%d",&n);

    j=sqrt(n);

    for(i=2;i<=j;i++)

        if(n%i==0) break;

        if(i>j) printf("%d shi sushu.\n",n);

        else

            printf("%d bushi sushu.\n",n);

   getch();

}
